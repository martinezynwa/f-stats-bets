@baseUrl = http://localhost:3001/external


### Fetch and insert Leagues, Team, Fixtures for single season
## params - externalLeagueId, season
POST {{baseUrl}}/init-leagues
Content-Type: application/json

{
  "externalLeagueIds": [15],
  "season": 2025
}

### Fetch and insert Leagues, Team, Fixtures for single season
## need to have the league-data.ts file set
## params - season
POST {{baseUrl}}/init-supported-leagues
Content-Type: application/json

{
  "season": 2025
}


### Fetch leagues
## params - externalLeagueId, season
GET {{baseUrl}}/leagues
Content-Type: application/json

{
  "externalLeagueId": 32,
  "season": 2024
}

### Fetch and insert league
## params - externalLeagueId, season
POST {{baseUrl}}/insert-league
Content-Type: application/json

{
  "externalLeagueId": 15,
  "season": 2025
}

### Fetch teams
## params - externalLeagueId, season
GET {{baseUrl}}/teams
Content-Type: application/json

{
  "externalLeagueId": 32,
  "season": 2024,
  "leagueId": "dfa327c4-c847-4420-bfdb-3921d11d7b5c"
}

### Fetch and insert teams
## params - externalLeagueId, season, leagueId
POST {{baseUrl}}/insert-teams
Content-Type: application/json

{
  "externalLeagueId": 15,
  "season": 2025,
  "leagueId": "d6dd86cb-6a43-4587-9472-b5b863e1f454"
}

### Fetch fixtures
## params - externalLeagueIds, ?season, ?dateFrom, ?dateTo
GET {{baseUrl}}/fixtures
Content-Type: application/json

{
  "externalLeagueIds": [32],
  "season": 2024,
  "dateFrom": "2025-06-07",
  "dateTo": "2025-06-07"
}

### Fetch and insert fixtures
## params - externalLeagueIds, ?season, ?dateFrom, ?dateTo
POST {{baseUrl}}/insert-fixtures
Content-Type: application/json

{
  "externalLeagueIds": [15],
  "season": 2025,
  "dateFrom": "2025-06-06",
  "dateTo": "2025-06-30"
}


